{
  "name": "@et/markdown",
  "description": "A markdown rendere for the E.Tutorial",
  "version": "0.1.0",
  "main": "lib/index.js",
  "author": "David Sichau",
  "repository": "Educational-Engineering/markdown",
  "license": "MIT",
  "scripts": {
    "compile": "babel src --out-dir lib",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prepublish": "npm run compile",
    "lint": "eslint .",
    "test": "babel-node ./node_modules/.bin/isparta cover _mocha"
  },
  "devDependencies": {
    "babel-cli": "*",
    "babel-preset-es2015-node4": "*",
    "coveralls": "*",
    "chai": "*",
    "isparta": "*",
    "mocha": "*",
    "sinon": "*",
    "eslint": "^2.10.2",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.1.1"
  },
  "eslintConfig": {
    "extends": [
      "airbnb"
    ],
    "rules": {
      "indent": [
        2,
        4
      ]
    }
  },
  "dependencies": {
    "cheerio": "^0.20.0",
    "jquery": "^3.0.0",
    "jsdom": "^9.2.1",
    "markdown-it": "^7.0.0",
    "markdown-it-anchor": "^2.5.0",
    "underscore": "^1.8.3"
  }
}
